{
  "name": "theia-frontend-clean-base",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "analyze": "ANALYZE=true yarn build",
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "test": "jest",
    "test:config": "jest src/config/__tests__/ --testPathIgnorePatterns",
    "integration-test": "start-server-and-test start http://localhost:3000 'cypress run'",
    "lint": "eslint 'src/**/*.{js,jsx,ts,tsx}'",
    "format:check": "prettier --check \"**/*.{js,jsx,ts,tsx,css,scss,json,md}\"",
    "format:check:staged": "pretty-quick --staged --check",
    "format:write": "prettier --write \"**/*.{js,jsx,ts,tsx,css,scss,json,md}\"",
    "prepare": "husky install",
    "postinstall": "yarn run typechain",
    "typechain": "typechain --out-dir src/config/abi/types --target=ethers-v5 \"src/config/abi/*.json\""
  },
  "dependencies": {
    "next": "12.1.0",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "@sentry/nextjs": "^6.17.9",
    "styled-components": "^5.3.0",
    "@theia-my/uikit": "^1.0.0",
    "lodash": "^4.17.21",
    "@binance-chain/bsc-connector": "^1.0.0",
    "@web3-react/core": "^6.1.9",
    "@ethersproject/abi": "^5.5.0",
    "@ethersproject/address": "^5.5.0",
    "@ethersproject/bignumber": "^5.5.0",
    "@ethersproject/bytes": "^5.5.0",
    "@ethersproject/constants": "^5.5.0",
    "@ethersproject/contracts": "^5.5.0",
    "@ethersproject/experimental": "^5.5.0",
    "@ethersproject/providers": "^5.5.2",
    "@ethersproject/strings": "^5.5.0",
    "@ethersproject/units": "^5.5.0",
    "@web3-react/core": "^6.1.9",
    "@web3-react/injected-connector": "^6.0.7",
    "@web3-react/walletconnect-connector": "6.2.4",
    "@web3-react/abstract-connector": "^6.0.7",
    "@pancakeswap/sdk": "^2.4.0",
    "@next/bundle-analyzer": "^12.0.7",
    "bignumber.js": "^9.0.0",
    "react-transition-group": "^4.4.1",
    "swr": "^1.1.2",
    "react-icons": "^4.3.1",
    "aos": "^3.0.0-beta.6",
    "sass": "^1.49.8",
    "bootstrap": "^5.1.3",
    "react-icons": "^4.3.1",
    "prettier": "^2.3.0"
  },
  "devDependencies": {
    "@types/node": "17.0.21",
    "@types/react": "17.0.39",
    "eslint": "8.10.0",
    "eslint-config-next": "12.1.0",
    "typescript": "4.6.2",
    "@types/lodash": "^4.14.178",
    "@types/styled-components": "^5.1.7",
    "@types/react-transition-group": "^4.4.1",
    "@pancakeswap/eslint-config-pancake": "^1.1.0"
  }
}
